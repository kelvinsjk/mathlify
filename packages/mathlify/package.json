{
	"name": "mathlify",
	"version": "0.0.26",
	"description": "bringing mathematics into the digital age",
	"source": "src/index.ts",
	"type": "module",
	"main": "lib/index.mjs",
	"types": "lib/types.d.ts",
	"scripts": {
		"dev": "parcel watch",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore",
		"build": "parcel build",
		"test": "uvu -r tsm src/__tests__",
		"test:debug": "uvu -r tsm src/__tests__/demos",
		"test:coverage": "c8 --include=src npm test"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kelvinsjk/mathlify.git"
	},
	"files": [
		"lib/**/*"
	],
	"keywords": [
		"math"
	],
	"author": "Kelvin Soh",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/kelvinsjk/mathlify/issues"
	},
	"homepage": "https://github.com/kelvinsjk/mathlify#readme",
	"devDependencies": {
		"@parcel/core": "^2.5.0",
		"@parcel/packager-ts": "^2.5.0",
		"@parcel/transformer-typescript-types": "^2.5.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"c8": "^7.10.0",
		"eslint": "^8.0.1",
		"parcel": "^2.5.0",
		"prettier": "^2.4.1",
		"tsm": "^2.1.2",
		"typescript": "^4.4.4",
		"uvu": "^0.5.2"
	}
}
