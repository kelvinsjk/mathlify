{
	"name": "mathlify",
	"type": "module",
	"version": "0.4.0",
	"description": "A computer algebra system with a focus on presentation",
	"author": "Kelvin Soh",
	"license": "MIT",
	"source": "./src/index.js",
	"main": "./src/index.js",
	"types": "./types/index.d.ts",
	"module": "./src/index.js",
	"exports": {
		".": {
			"default": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./classes": {
			"default": "./src/submodules/classes/index.js",
			"types": "./types/submodules/classes/index.d.ts"
		},
		"./working": {
			"default": "./src/submodules/working/index.js",
			"types": "./types/submodules/working/index.d.ts"
		}
	},
	"scripts": {
		"prepare": "pnpm run build",
		"prepublishOnly": "pnpm run test && pnpm run lint",
		"preversion": "pnpm run lint",
		"version": "pnpm run format && git add -A src",
		"postversion": "git commit -m 'new version' && git push && git push --tags",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ignore-path .gitignore",
		"vdev": "vite dev",
		"dev": "vitest",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest --config ./vitest.config.ts --coverage",
		"testLegacy": "vitest --config ./vitest-legacy.config.ts --coverage",
		"fractions": "vitest _expression",
		"types": "tsc",
		"cleanTypes": "rm -rf ./types && tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kelvinsjk/mathlify.git"
	},
	"files": [
		"lib/**/*"
	],
	"keywords": [
		"math"
	],
	"bugs": {
		"url": "https://github.com/kelvinsjk/mathlify/issues"
	},
	"homepage": "https://github.com/kelvinsjk/mathlify#readme",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"@vitest/coverage-v8": "^1.2.2",
		"eslint": "^8.56.0",
		"prettier": "^3.2.4",
		"temml": "^0.10.22",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vitest": "^1.2.2"
	}
}
